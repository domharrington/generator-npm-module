{
  "name": "<%= moduleName %>",
  "description": "<%= moduleDescription %>",
  "version": "0.0.0",
  "dependencies": {},
  "scripts": {
    "lint": "eslint -f unix .",
    "inspect": "jsinspect",
    "pretest": "npm run lint && npm run inspect",
    "test": "nyc mocha",
    "posttest": "nyc check-coverage --statements 90 --branches 70 --functions 90 --lines 90"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "<%= fullName %> <<%= emailAddress %>>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:<%= authorName %>/<%= moduleName %>.git"
  }
}
